    "clsx": "^2.1.1",
    "firebase": "^12.3.0",
    "lucide-vue-next": "^0.544.0",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
